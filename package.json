{
  "name": "monimejs",
  "version": "0.0.2",
  "description": "Unofficial lightweight and easy js/ts SDK for interacting with monime's endpoints.",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "biome check --write .",
    "format:check": "biome check .",
    "typecheck": "tsc --noEmit",
    "build": "npm run build:js && npm run build:types",
    "build:js": "esbuild src/index.js --bundle --format=esm --outfile=dist/index.js --target=node20 --minify --tree-shaking=true --external:valibot",
    "build:types": "cp src/index.d.ts dist/index.d.ts",
    "build:clean": "rm -rf dist && npm run build",
    "prepublishOnly": "npm run build:clean",
    "release": "npm publish --provenance --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doxalabs/monimejs.git"
  },
  "keywords": [
    "mobile money",
    "monime",
    "orange money",
    "afri money",
    "monimejs"
  ],
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Ben",
      "email": "bnjox@users.noreply.github.com",
      "url": "https://github.com/bnjox"
    }
  ],
  "maintainers": [
    {
      "name": "Ben",
      "email": "bnjox@users.noreply.github.com",
      "url": "https://github.com/bnjox"
    }
  ],
  "bugs": {
    "url": "https://github.com/doxalabs/monimejs/issues"
  },
  "homepage": "https://github.com/doxalabs/monimejs#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/node": "^25.3.1",
    "esbuild": "^0.27.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "valibot": "^1.2.0"
  }
}
