{
  "name": "monimejs",
  "version": "0.0.0-alpha.1",
  "description": "Unofficial lightweight and easy js/ts SDK for interacting with monime's endpoints.",
  "main": "dist/index.js",
  "type": "module",
  "engineStrict": true,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "biome check --write .",
    "build": "npm run build:js && npm run build:types",
    "build:js": "esbuild src/index.ts --bundle --format=esm --outdir=dist --external:all --sourcemap",
    "build:types": "tsc --emitDeclarationOnly",
    "build:clean": "rm -rf dist && npm run build",
    "prepublishOnly": "npm run build:clean",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atisans/monimejs.git"
  },
  "keywords": [
    "mobile money",
    "monime",
    "orange money",
    "afri money",
    "q money"
  ],
  "author": "The Artisans",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Sparrow",
      "email": "bnjox@users.noreply.github.com",
      "url": "https://github.com/bnjox"
    }
  ],
  "maintainers": [
    {
      "name": "Sparrow",
      "email": "bnjox@users.noreply.github.com",
      "url": "https://github.com/bnjox"
    }
  ],
  "bugs": {
    "url": "https://github.com/atisans/monimejs/issues"
  },
  "homepage": "https://github.com/atisans/monimejs#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.10.1",
    "esbuild": "^0.27.0",
    "typescript": "^5.9.3"
  }
}
