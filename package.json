{
  "name": "monimejs",
  "version": "0.0.2",
  "description": "Unofficial lightweight and easy js/ts SDK for interacting with monime's endpoints.",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "biome check --write .",
    "format:check": "biome check .",
    "build": "npm run build:js && npm run build:types",
    "build:js": "esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js --target=node20 --minify --tree-shaking=true --external:valibot",
    "build:types": "dts-bundle-generator -o dist/index.d.ts src/index.ts --no-banner",
    "build:clean": "rm -rf dist && npm run build",
    "prepublishOnly": "npm run build:clean",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atisans/monimejs.git"
  },
  "keywords": [
    "mobile money",
    "monime",
    "orange money",
    "afri money",
    "monimejs"
  ],
  "author": "The Artisans",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Ben",
      "email": "bnjox@users.noreply.github.com",
      "url": "https://github.com/bnjox"
    }
  ],
  "maintainers": [
    {
      "name": "Ben",
      "email": "bnjox@users.noreply.github.com",
      "url": "https://github.com/bnjox"
    }
  ],
  "bugs": {
    "url": "https://github.com/atisans/monimejs/issues"
  },
  "homepage": "https://github.com/atisans/monimejs#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.10.1",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.27.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "valibot": "^1.2.0"
  }
}
